	if ( sqlca.sqlcode == 0 ) {		/* OK */
		ret = RTN_OK;
	} else {
		if ( sqlca.sqlcode == MYAPP_RTN_DUP ) {	// duplicate
			//sprintf( G_msg,
			//		"依頼情報(irai)は既に存在しています。"
			//		"BML受付年月日[%s]依頼書NO[%s]依頼世代NO[%d]項目CD[%s]"
			//		,iri->utkymd, iri->irino, iri->zssdino, iri->kmkcd );
			ret=MYAPP_RTN_DUP;
		} else {
			/* ERROR */
			sprintf( G_msg,
				"依頼情報(irai)の INSERT に失敗しました。"
				"BML受付年月日[%s]依頼書NO[%s]依頼世代NO[%d]項目CD[%s]"
				,iri->utkymd, iri->irino, iri->zssdino, iri->kmkcd );
