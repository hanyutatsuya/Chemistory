	FROM	irai
	WHERE	utkymd	= :hirai.utkymd
	AND		irino	= :hirai.irino
--	AND		knsgrp	= ' '			// 依頼展開での検査G付加に伴い条件外
	AND		kmkcd	= :hirai.kmkcd
