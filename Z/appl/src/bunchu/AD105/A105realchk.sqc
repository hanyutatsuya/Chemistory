		SYOUGOPARA,
		KEISANPARA,
		PER100PARA,
		TOUSEKPARA,
		KISYAKPARA,
		KOUSEIPARA,
		SYOUNGPARA,
		SOUKN2PARA,
		FUKAPTPARA,
		YOBIPARA01,
		YOBIPARA02,
		YOBIPARA03,
		YOBIPARA04,
		YOBIPARA05,
		KSNDH
    ) VALUES (
		:knskisymd,
		:sriymd,
		:kntno,
		:kntketflg,
		:knsgrp,
		:kmkcd,
		:knssu,
		:knssueda,
		:utkymd,
